     <% include ../partials/header %>
<%let states = ["AK", "AL", "AR", "AZ", "CA", "CO", "CT", "DC",
    "DE", "FL", "GA", "HI", "IA", "ID", "IL", "IN", "KS", "KY", "LA",
    "MA", "MD", "ME", "MI", "MN", "MO", "MS", "MT", "NC", "ND", "NE",
    "NH", "NJ", "NM", "NV", "NY", "OH", "OK", "OR", "PA", "RI", "SC",
    "SD", "TN", "TX", "UT", "VA", "VT", "WA", "WI", "WV", "WY"]%>
<form action = '/guests/new' method = 'POST'>
     <label>First Name: </label><input name = 'fname' tyoe = 'text'></input><br>
     <label>Last Name: </label><input name = 'lname' type = 'text'></input><br>
     <label>Email: </label><input name = 'email' type = 'text'></input><br>
     <label>Phone: </label><input name = 'phone' type = 'text'></input><br>
     <label>Street Address:  </label><input name = 'address' type = 'text'></input><br>
     <label>City: </label><input name = 'city' type = 'text'></input><br>
     <label>State: </label><select name = 'state'>
    <div id = 'dropdown'> <%states.forEach(state => {%>
     <option value = '<%=state%>'><%=state%></option>
     <%});%>
     </select><br></div>
     <label>Zip Code: </label><input name = 'zip' type = 'text'></input>
</form>


    <% include ../partials/footer %>
